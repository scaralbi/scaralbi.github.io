body{color:var(--body-fg-color);background-color:var(--body-bg-color);font-family:sans-serif;line-height:1.5em}p{margin:1em 0}pre{padding:0 .5em;overflow-x:auto}p>code{background-color:var(--code-bg-color)}blockquote{border-left:3px solid var(--body-fg-color);color:var(--body-fg-color-deemphasize);padding-left:1em}.onionskin-details{cursor:pointer;text-decoration:underline}.onionskin-doublink{border-bottom:1px solid var(--body-fg-color);border-left:1px solid var(--body-fg-color);padding-left:2px}.onionskin-infobox{padding:.125em 1em}.onionskin-infobox-info{background-color:var(--infobox-info-bg-color)}.onionskin-infobox-warn{background-color:var(--infobox-warn-bg-color)}.onionskin-infobox-error{background-color:var(--infobox-error-bg-color)}a{color:var(--link-unvisited-color);text-decoration:none}a:visited{color:var(--link-visited-color)}a:hover{background-color:var(--link-hover-bg-color)}@media screen and (max-width:45rem){body#onionskin-full-frame{display:block}aside#onionskin-left-sidebar{display:block}main#onionskin-main-content{display:block}}@media screen and (min-width:45rem){body#onionskin-full-frame{display:grid;grid-template-columns:18rem 1fr}aside#onionskin-left-sidebar{grid-column:1;position:fixed;top:0;left:0;overflow-y:scroll;height:100vh;max-width:18rem}aside#onionskin-left-sidebar div.onionskin-left-sidebar-spacer{padding-bottom:2em}main#onionskin-main-content{grid-column:2}}@media screen and (min-width:90rem){body#onionskin-full-frame{grid-template-columns:22rem 1fr}aside#onionskin-left-sidebar{max-width:22rem}}section.sitenav{display:flex;justify-content:center;margin:1em}section.sitenav div.sitenav-links-container{text-align:center}section.sitenav div.sitenav-links-container ul{padding:1em 0;margin:0;list-style-type:none}section.sitenav div.sitenav-links-container ul li{padding:0;margin:0 1em;display:inline-block}nav#TableOfContents{font-size:.85rem;line-height:1.1rem}nav#TableOfContents ul{list-style:none;padding:0 0 0 1em}.onionskin-toc-page-title{font-weight:700;text-align:center}html{-webkit-text-size-adjust:none}.grid-fix-overflow-min-width-0{min-width:0}